    case 1:
    class accholder{
constructor(accnum,name,age,balance){
    this.accnum=accnum
    this.name=name
    this.age=age
    this.balance=balance

}

dsplay(){
    console.log(`acc no:`,this.accnum)
    console.log(`name:`,this.name)
    console.log(`age:`,this.age)
    console.log(`bal`,this.balance)
    console.log("  ")
}
}
var obj=new accholder(211010,"kush",20,100000000)
obj.dsplay()
var obj1=new accholder(211008,"jeren",20,1000000000)
