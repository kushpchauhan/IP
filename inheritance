case 2:
class calculator{

constructor(len,widht){
    this.len=len
    this.widht=widht
}
}
class rect extends calculator{
    constructor(radius,len,widht){
        super(len,widht)
        this.radius=radius
    }
    dispalyarearect(){

        return console.log(this.len*this.widht)
    }

}

class circleArea extends rect{
   
circle(){
    return console.log(Math.PI*this.radius*this.radius)
}

}
var obj=new circleArea(5,4,10)
obj.dispalyarearect()
obj.circle()


    default:
        break;
}


